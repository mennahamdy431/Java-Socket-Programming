#include <iostream>
using namespace std;
int main() {
  int date;
  cout << "enter the date \n";
  cin >> date;
  if (date > 0 && date < 32) {
    cout << "date is valid";
    if (date == 13 || date == 9)
      cout << " today is special ";
  }
  else {
    cout << "date is unvalid";
  }
  return 0;
}
