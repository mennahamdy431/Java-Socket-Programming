#include <iostream>
using namespace std;
int main() {
  char input;
  switch (input='b') {
  case 'a': {
      cout << "excellent";
      break;
    }
    case 'b': {
      cout << "very good";
      break;
    }
    case 'c': {
      cout << "good";
      break;
    }
    case 'd': {
      cout << "not bad";
      break;
    }
  }
  
  return 0;
}








